<header class="navbar navbar-fixed-top navbar-inverse">
      <div class="navbar-inner">
        <div class="container">
          <%= link_to "161", root_path, id: "logo" %>
          <nav>
            <ul class="nav pull-right">
              <li><%= link_to "Home", root_path %></li>
              <li><%= link_to "About", about_path %></li>
                <%if signed_in? %>
                  <li><%= link_to "My School List", school_items_path(current_user.school_id) %></li>
                  <li><%= link_to "Users", school_users_path(current_user.school_id) %></li>
                  <li><%= link_to "My Profile", user_path(current_user)%></li>
                  <li><%= link_to "Recent Activity", feed_user_path(current_user) %></li>
                  <li><%= link_to "Sign out", signout_path, method: "delete" %></li>
                <% else %>
                  <li><%= link_to "Sign in", signin_path %></li>
                <% end %>
            </ul>
          </nav>
        </div>
      </div>
    </header>